<?xml version="1.0"?>
<configuration>
  <configSections>
    <section name="G.Settings" type="G.Core.Setting.NameValueSectionHandler, G.Core" />
  </configSections>
  <G.Settings>
    <add key="NameMapper" value="G.Core.Text.UnderlineNameMapper, G.Core" />
    <add key="DataBase" value="@Access : @~test.mdb" />
  </G.Settings>
  <startup useLegacyV2RuntimeActivationPolicy="true">
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0"/>
  </startup>
  <runtime>
    <legacyCorruptedStateExceptionsPolicy enabled="true" />
  </runtime>
</configuration>
